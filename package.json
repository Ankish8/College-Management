{
  "name": "attendance-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:accessibility": "playwright test --project=accessibility",
    "e2e:performance": "playwright test --project=performance",
    "e2e:security": "playwright test tests/security/",
    "e2e:regression": "playwright test tests/regression/",
    "e2e:uat": "playwright test tests/uat/",
    "test:all": "npm run test:ci && npm run e2e",
    "test:quality": "npm run test:ci && npm run e2e:accessibility && npm run e2e:performance && npm run e2e:security",
    "db:seed": "tsx src/lib/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "analyze": "ANALYZE=true npm run build",
    "bundle-analyzer": "npx @next/bundle-analyzer",
    "prepare": "husky install",
    "check-all": "npm run type-check && npm run lint:check && npm run test:ci",
    "quality:report": "node scripts/generate-quality-report.js",
    "quality:dashboard": "npm run test:quality && npm run quality:report"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tabler/icons-react": "^3.34.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "prisma": "^6.11.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "react-hotkeys-hook": "^5.1.0",
    "recharts": "^2.15.4",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.3",
    "vaul": "^1.1.2",
    "zod": "^4.0.5",
    "zustand": "^5.0.2",
    "web-push": "^3.6.7",
    "immer": "^10.1.1",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.8",
    "@react-spring/web": "^9.7.3",
    "framer-motion": "^11.11.17",
    "react-intersection-observer": "^9.13.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/jest": "^29.5.12",
    "@testing-library/react": "^14.2.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@playwright/test": "^1.40.0",
    "@axe-core/playwright": "^4.8.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "@svgr/webpack": "^8.1.0",
    "@types/web-push": "^3.6.3",
    "next-pwa": "^5.6.0",
    "workbox-webpack-plugin": "^7.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.0",
    "@lhci/cli": "^0.12.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run check-all"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}